    public static double calculate(){
        double final_value=0;
        double term1=((20*sy1*sy2*sy3*sy4*h1*pc1)+(rbc5*100000000*0.6))/(100000000*rbc5*pfr5);
        double term2=((20*sy1*sy2*sy3*h1*pc1)+(rbc4*1000000))/(1000000*pfr4*rbc4);
        double term3=(runt3 + 170);
        double term4=((13*sy1*pc1*h1)+(rbc2*100))/(pfr2*rbc2*100);
        final_value=term1+term2+term3+term4;
        System.out.println("Computing Minimum value for sy1 = "+sy1+" sy2 = "+sy2+" sy3 = "+sy3+" sy4 = "+sy4+" sy5 = "+sy5+" h1 = "+h1+" pc1 = "+pc1+" pfr5 = "+pfr5+" pfr4 = "+pfr4+" rbc5 = "+rbc5+" rbc4= "+rbc4+" rbc2="+rbc2);
        System.out.println("Value Computed is:"+final_value);
        return final_value;
    }
